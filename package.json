		"redis": "^4.6.13",
		"schema-openapi": "^0.33.4",
		"telegraf": "^4.16.3",
		"ts-node": "^10.9.2",
		"typescript": "^5.4.2"
	},
	"devDependencies": {
		"@tsconfig/node20": "^20.1.2",
